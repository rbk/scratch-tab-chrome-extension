{
  "name": "Scratch Tab",
  "version": "0.0.5",
  "manifest_version": 2,
  "permissions" : [
    "storage",
    "tabs",
    "browser"
  ],
  "description": "Tab for temporary text space. Syncs for all new tabs",
  "homepage_url": "http://richardkeller.net",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background" : {
  },
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  }
}
